{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js"]
    },
    {
      "type": "node-terminal",
      "name": "Run Script: serve",
      "request": "launch",
      "command": "npm run serve",
      "cwd": "${workspaceFolder}"
    }
  ]
}
