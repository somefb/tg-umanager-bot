{
  "name": "telegram-umanager-bot",
  "version": "0.0.1",
  "description": "helps to manage users",
  "main": "./build/index.js",
  "scripts": {
    "start": "node build/index.js --https",
    "test": "jest",
    "serve": "cross-env DEBUG=true DEV=true ./node_modules/.bin/ts-node src/index.ts --inspect",
    "serve-prod": "cross-env DEBUG=true VERBOSE=false ./node_modules/.bin/ts-node src/index.ts",
    "test-google": "./node_modules/.bin/ts-node src/googleDrive/check.ts",
    "lint": "npm run lint-es && npm run lint-ts",
    "lint-es": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json}",
    "lint-ts": "tsc --skipLibCheck --incremental --noEmit",
    "build-old": "tsc --listEmittedFiles --incremental",
    "build-dev": "./node_modules/.bin/webpack --mode development",
    "build-prod": "./node_modules/.bin/webpack --mode production",
    "certificate": "node ssl/generateCert.js",
    "deploy": "npm run build-prod && eb deploy",
    "logs": "eb logs Tgbotumanager-env --all",
    "postinstall": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yegorich555/telegram-umanager-bot.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yegorich555/telegram-umanager-bot/issues"
  },
  "homepage": "https://github.com/Yegorich555/telegram-umanager-bot#readme",
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/jest-expect-message": "^1.0.3",
    "@types/node": "^14.14.42",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "editorconfig": "^0.15.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "selfsigned": "^1.10.8",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.2.0",
    "ts-node": "^9.1.1",
    "typegram": "^3.3.1",
    "typescript": "^4.2.4",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "googleapis": "^67.1.1"
  }
}
